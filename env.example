# GenZSpace Environment Variables
# Copy this file to .env and fill in your actual values

# Firebase Configuration
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id

# Google Services
GOOGLE_SERVICES_JSON_PATH=android/app/google-services.json
GOOGLE_SERVICE_INFO_PLIST_PATH=ios/Runner/GoogleService-Info.plist

# API Keys (if you add any external services)
# OPENAI_API_KEY=your_openai_key_here
# STRIPE_PUBLISHABLE_KEY=your_stripe_key_here
# MAPS_API_KEY=your_google_maps_key_here

# App Configuration
APP_NAME=GenZSpace
APP_VERSION=1.0.0
DEBUG_MODE=true
